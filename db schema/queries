SELECT op.oid, op.oprname, ns.nspname, pg_get_userbyid(op.oprowner) AS opowner, op.oprkind, op.oprcanhash, op.oprleft, op.oprright, lt.oid AS ltoid, lt.typname AS lefttype, rt.oid AS rtoid, rt.typname AS righttype, ltns.nspname AS ltschema, rtns.nspname AS rtschema, op.oprcom, op.oprnegate, op.oprcanmerge, prc.proname AS operproc, nsc.nspname AS operns, prr.proname AS restproc, nsr.nspname AS restns, prj.proname AS joinproc, nsj.nspname AS joinns, obj_description(op.oid) AS description FROM pg_operator op LEFT JOIN pg_namespace ns ON ns.oid=op.oprnamespace LEFT JOIN pg_type lt ON lt.oid=op.oprleft LEFT JOIN pg_namespace ltns ON ltns.oid=lt.typnamespace LEFT JOIN pg_type rt ON rt.oid=op.oprright LEFT JOIN pg_namespace rtns ON rtns.oid=rt.typnamespace LEFT JOIN pg_proc prc ON prc.oid=op.oprcode LEFT JOIN pg_namespace nsc ON nsc.oid=prc.pronamespace LEFT JOIN pg_proc prr ON prr.oid=op.oprrest LEFT JOIN pg_namespace nsr ON nsr.oid=prr.pronamespace LEFT JOIN pg_proc prj ON prj.oid=op.oprjoin LEFT JOIN pg_namespace nsj ON nsj.oid=prj.pronamespace

